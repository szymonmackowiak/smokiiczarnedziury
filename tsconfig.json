{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verbatimModuleSyntax": true,
    "paths": {
      "src": [
        "./src"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist"
  ]
}
